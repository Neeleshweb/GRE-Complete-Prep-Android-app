<ScrollView android:id="@+id/comeonman"
    android:layout_width="357dp"
    android:layout_height="wrap_content"
    android:background="@drawable/back"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:background="@drawable/back"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.homefilescenter.dynamicrankingplatform.mega.homefilescenterofficial.Contactus">

        <TextView
            android:id="@+id/f1"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="19dp"
            android:text="1)-ALL THE 3 SECTIONS AVAILABLE, THAT IS QUANT, VERBAL, TEST."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="10dp"/>

        <TextView
            android:id="@+id/f2"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="19dp"
            android:text="2)-THE CHEAPEST APP WHICH PROVIDES COMPLETE PREPARATION."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>
        <TextView
            android:id="@+id/f3"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="3)-WORD LISTS, READING COMPREHENSIONS, TEXT COMPLETION AND SENTENCE EQUIVALENCE, ALL FOUR SUB-SECTIONS OF VERBAL COVERED"
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>

        <TextView
            android:id="@+id/f4"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="4)-QUANTITATIVE COMPARISON, NUMERIC ABILITY, PROBLEM SOLVING, ALL THE 3 SECTIONS OF QUANT COVERED."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>
        <TextView
            android:id="@+id/f5"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="5)-MORE THAN 3000 WORDS FOR VOCAB, EACH WITH IT'S MEANING AND AN EXAMPLE OF IT'S USAGE IN A SENTENCE FOR TOP 1500 WORDS."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>

        <TextView
            android:id="@+id/f6"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="6)-A PREDICTED SCORE SECTION, WHICH HELPS THE STUDENT TO KNOW ABOUT HIS/HER CURRENT LEVEL OF PREPARATION."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>

        <TextView
            android:id="@+id/f7"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="7)-A UNIVERSITY MATCHER SECTION, WHICH HELPS TO PREDICT WHICH UNIVERSITY MIGHT THE STUDENT GET AT HIS PREDICTED SCORE."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>
        <TextView
            android:id="@+id/f8"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="8)-A UNIVERSITY VAULT SECTION ENLISTING SOME OF THE BEST UNIVERSITIES FOR DOING YOUR MASTERS."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>

        <TextView
            android:id="@+id/f9"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="9)-FIVE QUIZ EACH CONSISTING OF 10 QUESTIONS, FOR TESTING YOUR PREPARATION LEVEL AND PREDICTING YOUR SCORE IN BOTH SECTIONS."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>
        <TextView
            android:id="@+id/f10"
            android:layout_width="323dp"
            android:layout_height="wrap_content"

            android:textColor="#fff"

            android:textStyle="bold"
            android:textSize="19dp"
            android:text="10)-QUESTIONS FROM ALL THE SECTION OF MATHS IS COVERED LIKE ALGEBRA, GEOMETRY, STATISTICAL MODELLING ."
            android:padding="16dp"
            android:gravity="center"

            android:background="@drawable/textview2"
            android:layout_marginTop="30dp"/>



    </LinearLayout>
</ScrollView>
